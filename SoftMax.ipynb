   "source": [
    "import numpy as np\n",
    "npa = np.array\n",
    " \n",
    "def softmax(w, t = 1.0):\n",
    "    e = np.exp(npa(w) / t)\n",
    "    dist = e / np.sum(e)\n",
    "    return dist\n",
    " \n",
    " \n",
    "w = np.array([0.1,0.9])\n",
    "print softmax(w)\n",
    "\n",
    "w = np.array([-0.1,0.2])\n",
    "print softmax(w)\n",
    "\n",
    "w = np.array([0.9,-10])\n",
    "print softmax(w)"
   ]
  }
